#!/bin/bash

source build/SETTINGS

######################################################################################
#  Configure Findex Search Launcher
######################################################################################
echo -e "$LB$D Setting up Findex Search Launcher...$LB$END"
######################################################################################

git clone https://github.com/mdgaziur/findex $DIR_TMP/findex > /dev/null 2>&1

bash $DIR_TMP/findex/installer.sh -y
